<body>

    <div ng-controller="homeCtrl">
        <!--**************************************End of student action-->
        <form class="needs-validation" name="donationForm" ng-submit="submitDonation(donationForm)" novalidate>

            <div class="row vm-row">
                <div class="container vm-form-header">
                    <br />
                    <div class="row vm-row">
                        <div style="width:auto;height:auto;">
                            @*<label for="one" class="pointer-cursor">
                            click/toggle notification
                        </label>*@
                            <input type="checkbox" id="one" class="hidden" name="ossm">
                            <label for="one" class="alert-message">
                                <strong> <i class="fa fa-heart"></i> Attention</strong><br />
                                You are currently donating !!
                                <span class="close"></button>
                            </label>
                        </div>
                    </div>
                    <div class="StudentDonationText">
                        @{
                            if (studentData == null)
                            {
                                <table>
                                    <tr>
                                        <td colspan="4">No rows match found </td>
                                    </tr>
                                </table>
                            }
                            else
                            {
                                <p> @studentData.Firstname @studentData.Lastname</p>
                                <div style="line-height:20px;">
                                </div>
                            }
                        }
                    </div>
                    <div class="col">
                        <div class="Studentcontainer">
                            <div class="form-group">
                                <div class="Studentcontainerleft-col">
                                    @{
                                        if (studentData == null)
                                        {
                                            <table>
                                                <tr>
                                                    <td colspan="4">No rows match found </td>
                                                </tr>
                                            </table>
                                        }
                                        else if (studentData != null)
                                        {
                                            if (studentData.Picture != null)
                                            {
                                                var base64 = Convert.ToBase64String(studentData.Picture);
                                                var exptention = "jpg";
                                                var imgscr = "";
                                                switch (exptention)
                                                {
                                                    case "png":
                                                        imgscr = string.Format("data:image/" + exptention + ";base64,{0}", base64);
                                                        break;
                                                    case "tiff":
                                                        imgscr = string.Format("data:image/" + exptention + ";base64,{0}", base64);
                                                        break;
                                                    case "jpg":
                                                        imgscr = string.Format("data:image/" + exptention + ";base64,{0}", base64);
                                                        break;
                                                    case "gif":
                                                        imgscr = string.Format("data:image/" + exptention + ";base64,{0}", base64);
                                                        break;
                                                }
                                                <img src="@imgscr" style="height:auto; width:250px;" class="img-fluid" alt="PPS Foundation Banner" />
                                            }
                                        }
                                    }
                                </div>
                                <div class="Studentcontainerright-col">
                                    @{
                                        if (studentData == null)
                                        {
                                            <table>
                                                <tr>
                                                    <td colspan="4">No rows match found </td>
                                                </tr>
                                            </table>
                                        }
                                        else
                                        {
                                            <h5>Region : </h5>
                                            <h5>Current Institution Attending : @studentData.University </h5>
                                            <h5>Current Course : @studentData.Qualification </h5>
                                            <h4>More about my jounery</h4>
                                            <p>
                                                @studentData.WhatTheFundingWillBeUsedFor <br />
                                                @studentData.Bio<br />
                                            </p>
                                        }
                                    }
                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
