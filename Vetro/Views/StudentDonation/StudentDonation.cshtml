                            </div>
                            <!--<div class="col">
                        <img ng-click="selectDonation(67)" class="mandela donationButton" src="~/Assets/img/R67forMandelaDayButton.png">
                    </div>-->
                        </div>
                        <div>
                            @{
                                if (studentData == null)
                                {
                                    Context.Response.Redirect("~/Home/Index");
                                    @*@Html.ActionLink("Home", "Index");*@
                                }
                                else
                                {
                                    @*@Html.Hidden("Hid", studentData.Id)*@
                                    <input type="hidden" required ng-model="donation.StudentDetails.Id" name="@studentData.Id" ng-init="donation.StudentDetails.Id= @studentData.Id">
                                    @*<input required ng-model="donation.StudentDetails.Id" type="text" name='Id' value='@studentData.Id'>*@
                                    @*@Html.HiddenFor(m => m., new { @id = "hfName" })*@
                                    <p>@*@studentData.Id*@</p>
                                }
                            }

                        </div>
                        <div class="form-group">
